package calculate;
import java.util.ArrayList;


public class Inner {
	
	// We use this class and method to use for the check path from shortPath class.
	// It will be used inside an arrayList so that we can have an arrayList of arrayLists and search for different combinationts of flights
	//
	
	
	private ArrayList<Integer> inside;

	public ArrayList<Integer> getInside() {
		return inside;
	}

	public void setInside(ArrayList<Integer> inside) {
		this.inside = inside;
	}
	
	public String print() {
		
		String s = "";
		
		for (Integer t : inside) {
			s+= t.toString();
		}
		return s;
		
	}
	

}
